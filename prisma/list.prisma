model List {
  id                String   @id @default(cuid())
  title             String   
  defaultColor      Int?
  customColor       String?  
  order             Int      
  favorite          Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt

  folderId          String
  folder            Folder @relation(fields: [folderId], references: [id])

  @@map("lists")
}